{
	"name": "innoweb/silverstripe-googleanalytics",
	"description": "Adds Google Analytics to the site using Universal Analytics with event and download tracking as well as Google Tag Manager.",
	"type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
	"keywords": ["silverstripe", "event tracking", "analytics", "tag manager", "multisites"],
	"homepage": "http://github.com/xini/silverstripe-googleanalytics",
	"authors": [
	{
		"name": "Florian Thoma",
		"homepage": "http://www.innoweb.com.au"
	}
	],

	"require": {
		"silverstripe/framework": "^4",
        "unclecheese/display-logic": "^2.0.0"
	},
    "autoload": {
        "psr-4": {
            "Innoweb\\GoogleAnalytics\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client"
        ]
    },
    "replace": {
        "xini/silverstripe-googleanalytics": "self.version"
    }
}
